{
  "manifest_version": 3,
  "name": "Fast Events",
  "version": "1.0",
  "description": "Create calendar events from text or image",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "178586951197-spg9qap2s7b3glplrfc77cohgu1eapk8.apps.googleusercontent.com.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "https://www.googleapis.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Ctrl+Q"
      }
    }
  }
}
